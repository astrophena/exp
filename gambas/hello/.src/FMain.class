' Gambas class file

Public greeting As String

Public Sub GreetingForm_KeyPress()
  greeting = GreetingForm.Text
End

Public Sub Greet_Click()
  Message(GreetingForm.Text, "OK")
End

Public Sub ShowOSRelease_Click()
  Dim text As String[] = Split(File.Load("/etc/os-release"), gb.NewLine, "", True)
  Message(text.Join(gb.NewLine), "OK")
End
